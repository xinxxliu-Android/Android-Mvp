{
  /**
   * 线路/分组上的设备(杆塔/风机/太阳能板等)
   */
  devices: [
    {
      /**
* 设备所属分组 名
*
* xxxx线
* xxx工区
* xxxx工区
* xx2号网格
*/
      groupName: "xxx",
      /**
       * 设备所属分组id
       */
      groupGuid: "xxxx",
      /**
       * 分组类型
       * 杆塔 风机 光伏等
       * 其中：
       * 0：杆塔分组
       * 1：风机分组
       * 2：光伏分组
       * 3：网格分组(多种设备构成的分组，分组内可同时拥有多个设备)
       */
      groupType: 0,
      /**
   * 经度 纬度 海拔高度 台账位置
   */
      la: 11.11,
      lo: 11.11,
      al: 11.11,
      /**
       * 设备唯一识别码
       */
      deviceGuid: "",
      /**
     * 设备编号
     *  注意：该字段仅用于 设备排序
     */
      deviceNumber: "",
      /**
     * 设备名
     * 如：xxx号杆塔  或 xxx号风机  或 xxx号太阳能板 等
     * 设备完全限定名 此字段在重命名、工单使用时会进行展示
     * TODO 注意，目前该字段已使用
     */
      deviceName: "11",
        /**
     * 设备备注
     */
        deviceMarker: "xxxx",
        /**
          * 设备类型
          */
        deviceType: 1,

        /**
     * 电压等级 kv
     */
      voltageLevel: 111,
      /**
   * 设备 PMS编码
   */
      pmsCode: "111",
        /**
       * 设备朝向
       * 下发非精细巡检工单时，该值为必要值
       * 取值范围： 180 - -180
       * 取值基准： 磁北方向
       */
        radius: 155.6,
        /**
     *  设备精细巡检航迹
     *  Base64加密字符串
     *  TODO 暂不使用该字段
     */
        protocol: "xxxx",
    }
  ]
}